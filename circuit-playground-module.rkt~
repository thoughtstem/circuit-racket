(module circuit-racket racket
  (provide
   (all-from-out "./circuit-python-base.rkt")
   (all-from-out "./circuit-python.rkt")
   (all-from-out "./lights.rkt")
   (all-from-out "./sound.rkt")
   (all-from-out "./input.rkt")
   (all-from-out "./output.rkt")
   (all-from-out "./accel.rkt")
   (all-from-out "./servo.rkt")
   (all-from-out "./ir.rkt")
   (rename-out [py-begin begin])
   (rename-out [define-user-function define])
   #%module-begin)

  (require "./circuit-python-base.rkt")
  (require "./circuit-python.rkt")
  (require "./lights.rkt")
  (require "./sound.rkt")
  (require "./input.rkt")
  (require "./output.rkt")
  (require "./accel.rkt")
  (require "./servo.rkt")
  (require "./ir.rkt"))

